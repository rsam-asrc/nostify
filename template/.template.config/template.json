{
    "$schema": "http://json.schemastore.org/template",
    "author": "Travis Boyle",
    "classifications": [ "Azure", "Microservice" ],
    "identity": "Nostify.Csharp",
    "name": "Nostify Template",
    "shortName": "nostify",
    "placeholderFilename": ".placeholder",
    "symbols": {
        "aggregateName": {
            "type": "parameter",
            "isRequired": true,
            "FileRename": "_ReplaceMe_",
            "replaces":"_ReplaceMe_",
            "description": "Name of the Aggregate"
        },
        "port":{
            "type": "parameter",
            "isRequired": true,
            "replaces":"<ThePortNumber>",
            "description": "The port number the project will run on locally",
            "defaultValue": "CosmosEmulatorConnectionString"
        },
        "connectionString":{
            "type": "parameter",
            "isRequired": false,
            "replaces":"<YourConnectionStringHere>",
            "description": "The connection string or setting variable",
            "defaultValue": "CosmosEmulatorConnectionString"
        }
    }
  }